.parent {
display: grid;
grid-template-columns: repeat(16, 1fr);
grid-template-rows: repeat(9, 1fr);
grid-column-gap: 10px;
grid-row-gap: 10px;
}

.div1 { grid-area: 1 / 1 / 10 / 5; }
.div2 { grid-area: 1 / 13 / 10 / 17; }
.div3 { grid-area: 1 / 5 / 6 / 13; }
.div4 { grid-area: 6 / 5 / 10 / 9; }
.div5 { grid-area: 6 / 9 / 10 / 13; }